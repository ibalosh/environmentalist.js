{
  "name": "environmentalist.js",
  "version": "1.2.5",
  "description": "A simple Node.js app to be used with Slack for managing environment occupancy.",
  "license": "MITNFA",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "directories": {
    "lib": "./dist/index.js"
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.1.0",
    "@types/chai": "^4.1.4",
    "@types/mocha": "^8.0.3",
    "@types/node": "^12.12.6",
    "@types/node-localstorage": "^1.3.0",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "chai": "^4.1.2",
    "eslint": "^8.6.0",
    "mocha": "^9.1.3",
    "nconf": "^0.10.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "4.1.6"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.46.0",
    "@types/express": "^4.16.0",
    "axios": "^0.24.0",
    "body-parser": "^1.18.2",
    "express": "^4.17.2",
    "install": "^0.12.2",
    "moment": "^2.29.1",
    "node-localstorage": "^1.3.1",
    "slack-block-builder": "^2.4.2"
  },
  "scripts": {
    "compile": "npm run clean && npm run ts && npm run permissions",
    "permissions": "chmod +x ./dist/index.js",
    "clean": "rm -r -f ./dist",
    "ts": "node_modules/.bin/tsc",
    "test": "node_modules/mocha/bin/mocha --timeout 10000 --retries 1 -r ts-node/register test/**/*test.ts",
    "lint": "./node_modules/.bin/eslint src/**/*.ts",
    "dev": "nodemon",
    "start": "set -o allexport && source ./.env && set +o allexport && ts-node src/run.ts"
  },
  "author": "Igor Balos",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ibalosh/environmentalist.git"
  },
  "bugs": {
    "url": "https://github.com/ibalosh/environmentalist/issues"
  },
  "homepage": "https://github.com/ibalosh/environmentalist#readme"
}
